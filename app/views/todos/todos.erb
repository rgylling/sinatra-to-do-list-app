<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4 col-sm-12 text-center bg-white">
      <div>
        <h1 class="log-in"><%="#{current_user.username}'s todos"%></h1>
        <% @todos.each do |todo| %>
          <ul>
            <li>
              <a href="/todos/<%= todo.id %>"><%=todo.content%></a>
            </li>
          </ul>
        <% end %>

      </div>
    </div>
  </div>
</div>
</div>
